<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dao.mapper.exportCodeSave.ExportCodeSaveMapper">
    <insert id="add" parameterType="ExportCodeSave" databaseId="h2">
        insert into T_EXPORT_CODE(EXPORT_ID,HTML_CODE,JS_CODE,CREATE_TIME) values(#{exportId},#{htmlCode},#{jsCode},#{createTime})
    </insert>
    <select id="queryAsObject" parameterType="String" resultType="ExportCodeSave" databaseId="h2">
        select ID, HTML_CODE, JS_CODE,CREATE_TIME from T_EXPORT_CODE where EXPORT_ID = #{exportId}
    </select>
</mapper>