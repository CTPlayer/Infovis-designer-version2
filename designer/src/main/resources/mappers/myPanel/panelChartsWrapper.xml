<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="model.myPanel.PanelChartsWrapper">
    <insert id="insert" parameterType="PanelChartsWrapper" databaseId="h2">
        insert into T_PANEL_CHARTS (EXPORT_ID,CHART_ID,ORDER_ID) values (#{exportId},#{chartId},#{orderId})
    </insert>

    <delete id="delete" parameterType="PanelChartsWrapper" databaseId="h2">
        delete from T_PANEL_CHARTS
        <where>
            <if test="exportId != null and exportId !=''">
                EXPORT_ID = #{exportId}
            </if>
            <if test="chartId != null and chartId !=''">
                CHART_ID = #{chartId}
            </if>
        </where>
    </delete>

</mapper>
